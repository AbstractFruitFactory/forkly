    "title": "Fresh Garden Salad",
    "description": "A light and refreshing salad packed with seasonal vegetables and a zesty vinaigrette.",
    "instructions": [
      {
        "id": "salad-1",
        "text": "Wash and dry all vegetables thoroughly",
        "ingredients": [
          { "name": "mixed salad greens", "displayName": "mixed salad greens", "quantity": "6", "measurement": "cups" },
          { "name": "cherry tomatoes", "displayName": "cherry tomatoes, halved", "quantity": "1", "measurement": "cup" },
          { "name": "cucumber", "displayName": "cucumber, sliced", "quantity": "1", "measurement": "medium" },
          { "name": "red onion", "displayName": "red onion, thinly sliced", "quantity": "1/4", "measurement": "cup" }
        ]
      },
      {
        "id": "salad-2",
        "text": "Slice cucumber and red onion thinly"
      },
      {
        "id": "salad-3",
        "text": "Halve the cherry tomatoes"
      },
      {
        "id": "salad-4",
        "text": "In a small bowl, whisk together olive oil, balsamic vinegar, and honey",
        "ingredients": [
          { "name": "extra-virgin olive oil", "displayName": "extra-virgin olive oil", "quantity": "3", "measurement": "tablespoons" },
          { "name": "balsamic vinegar", "displayName": "balsamic vinegar", "quantity": "2", "measurement": "tablespoons" },
          { "name": "honey", "displayName": "honey", "quantity": "1", "measurement": "teaspoon" }
        ]
      },
      {
        "id": "salad-5",
        "text": "Combine all vegetables in a large bowl"
      },
      {
        "id": "salad-6",
        "text": "Drizzle with dressing just before serving",
        "ingredients": [
          { "name": "salt", "displayName": "salt", "quantity": "1/2", "measurement": "teaspoon" },
          { "name": "black pepper", "displayName": "black pepper, freshly ground", "quantity": "1/4", "measurement": "teaspoon" }
        ]
      },
      {
        "id": "salad-7",
        "text": "Toss gently and serve immediately"
      }
